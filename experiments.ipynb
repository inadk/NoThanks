class PenaltyPerCoins(Player):
    def __init__( self ):
        Player.__init__( self )
        self.setName( 'PnealtyPerCoins' )
    def take( self, card, state ):
        if self.coins <= 0:
            return True
        if card.coins >= ( self.penaltyWhenTake( card ) / 1.4 ):
            return True
        return False

